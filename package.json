{
  "name": "next-image-tagger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "setup": "npx prisma generate && npx prisma db push && npx prisma db seed",
    "prisma:seed": "npx prisma db seed",
    "prisma:migrate:test:env": "dotenv -e .env.test.local -- sh -c 'npx prisma generate'",
    "prisma:migrate:test:envx": "dotenv -e .env.test.local -- sh -c 'npx prisma migrate dev --name init'",
    "lint": "next lint",
    "test:cypress": "npm run prisma:migrate:test:env && npm run build && NODE_ENV=test start-server-and-test start 3000 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test:cypress",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@apollo/server": "^4.9.5",
    "@as-integrations/next": "^3.0.0",
    "@conform-to/react": "^0.9.1",
    "@conform-to/zod": "^0.9.1",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.6.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "graphql": "^16.8.1",
    "lucide-react": "^0.294.0",
    "next": "14.0.2",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
